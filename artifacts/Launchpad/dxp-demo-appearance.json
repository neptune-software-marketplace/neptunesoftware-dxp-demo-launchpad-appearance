		"enhancement": [
			"==OBJECT STRING==",
			"sap.n.Enhancement.BeforeStartup = function () {",
			"            console.log('Trigger Enhancement BeforeStartup');",
			"            console.log(`Enhancement Before Startup: Adding \"Change Appearance button\"...`);\r",
			"\r",
			"const btnThemeChange = new sap.m.Button(`btnThemeChange`, {\r",
			"    text: \"Change Appearance\",\r",
			"    visible: false,\r",
			"    press: function (e) {\r",
			"        popListAppearance.openBy(this);\r",
			"    },\r",
			"});\r",
			"AppCacheShellCustomRight.addItem(btnThemeChange);\r",
			"\r",
			"const btnEnableHelpAppearance = new sap.m.Button(`btnEnableHelpAppearance`, {\r",
			"    text: \"Disable Help\",\r",
			"    visible: false,\r",
			"    press: function (e) {\r",
			"        if (this.getText() === \"Disable Help\") {\r",
			"            document.documentElement.classList.add(\"nepDxpDemoDisableHelp\");\r",
			"            this.setText(\"Enable Help\");\r",
			"            sap.n.Shell.closeSidepanel();\r",
			"            \r",
			"        } else {\r",
			"            document.documentElement.classList.remove(\"nepDxpDemoDisableHelp\");\r",
			"            this.setText(\"Disable Help\");\r",
			"            sap.n.Shell.loadSidepanel(\"dxp_demo_appearance_help\", {\r",
			"                tabTitle: \"Help\"\r",
			"            });\r",
			"        }\r",
			"    },\r",
			"});\r",
			"AppCacheShellCustomRight.addItem(btnEnableHelpAppearance);\r",
			"\r",
			"const ListAppearance = new sap.m.List(\"ListAppearance\");\r",
			"const itemListAppearance = new sap.m.StandardListItem(\"itemListAppearance\", {\r",
			"    title: \"{name}\",\r",
			"    type: \"Active\",\r",
			"    press: function (oEvent, ui) {\r",
			"        const context = oEvent.oSource.getBindingContext();\r",
			"        if (context) {\r",
			"            const data = context.getObject();\r",
			"            AppCache.loadCustomLayouts({\r",
			"                layoutLight: data.layoutLight,\r",
			"                layoutDark: data.layoutDark,\r",
			"                updateUser: true,\r",
			"            });\r",
			"        }\r",
			"        popListAppearance.close();\r",
			"    },\r",
			"});\r",
			"\r",
			"const modelListAppearance = new sap.ui.model.json.JSONModel();\r",
			"ListAppearance.setModel(modelListAppearance);\r",
			"ListAppearance.bindAggregation(\"items\", {\r",
			"    path: \"/\",\r",
			"    template: itemListAppearance,\r",
			"    templateShareable: false,\r",
			"});\r",
			"const popListAppearance = new sap.m.Popover(\"popListAppearance\", {\r",
			"    placement: \"Bottom\",\r",
			"    showHeader: false,\r",
			"}).addStyleClass(\"sapUiSizeCompact\");\r",
			"popListAppearance.addContent(ListAppearance);\r",
			"dataAppearance = [{\r",
			"    name: 'Horizon',\r",
			"    layoutLight: 'light',\r",
			"    layoutDark: 'dark',\r",
			"}, {\r",
			"    name: 'Hurricanes',\r",
			"    layoutLight: '262c50c7-984a-4d31-9812-dd33221fde62',\r",
			"    layoutDark: '2a8b35fb-7d2a-430c-8215-0977a45ac92a',\r",
			"}, {\r",
			"    name: 'Kraken',\r",
			"    layoutLight: '6b3256f9-0ed7-4c45-aa34-32f31d0fe844',\r",
			"    layoutDark: '45d1b888-1952-44b3-9657-3b458438c406',\r",
			"}];\r",
			"\r",
			"modelListAppearance.setData(dataAppearance);\r",
			"\r",
			"window.dxpDemoAppearance = {\r",
			"    setThemeChangeVisibility: (visible) => {\r",
			"        btnThemeChange.setVisible(visible);\r",
			"        btnEnableHelpAppearance.setVisible(visible);\r",
			"    }\r",
			"};",
			"        };sap.n.Enhancement.BeforeLock = function (authType) {",
			"            console.log('Trigger Enhancement BeforeLock');",
			"            dxpDemoAppearance.setThemeChangeVisibility(false);",
			"        };sap.n.Enhancement.AfterLogout = function (authType) {",
			"            console.log('Trigger Enhancement AfterLogout');",
			"            dxpDemoAppearance.setThemeChangeVisibility(false);",
			"        };sap.n.Enhancement.BeforeUpdate = function () {",
			"            console.log('Trigger Enhancement BeforeUpdate');",
			"            dxpDemoAppearance.setThemeChangeVisibility(true);",
			"        };sap.n.Enhancement.SuccessGetMenu = function () {",
			"            console.log('Trigger Enhancement SuccessGetMenu');",
			"            setTimeout(() => {\r",
			"    sap.n.Shell.loadSidepanel(\"dxp_demo_appearance_help\", {\r",
			"        tabTitle: \"Help\"\r",
			"    });    \r",
			"}, 1000);",
			"        };"
		],
